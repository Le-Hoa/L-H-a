<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE DOCUMENT [
        <!ELELMENT document (heading, message)>
        <!ELELMENT heading (#PCDATA)>
        <!ELELMENT message (#PCDATA)>
]>
<document>
    <heading>
        Hello Form XML
    </heading>
    <message>
        This is an XML document !
    </message>
</document>

